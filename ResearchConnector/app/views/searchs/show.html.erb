<div class="page-header">
  <h3>Search Results</h3>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Research Field</th>
        <th>Research Field ID</th>
        <th>Research Need</th>
        <th>Title</th>
        <th>Submitter Name</th>
        <th>Email Address</th>
        <th>Preferred Contact Number</th>
        <th>Grant Amount</th>
        <th>Abstract</th>
        <th>More details</th>
        <th>Actions</th>
      </tr>

      <% @granttables.each do |granttable| %>
      <tr>
        <dt>Research Field</dt>
        <dt>Research Field ID</dt>
        <dt>Research Need</dt>
        <dt>Title</dt>
        <dt>Submitter Name</dt>
        <dt>Email Address</dt>
        <dt>Preferred Contact Number</dt>
        <dt>Grant Amount</dt>
        <dt>Abstract</dt>
        <dt>More details</dt>
        <div class="btn-group btn-group-justified">
          <a href="/granttables" class="btn btn-primary">Back</a>
        </div>
        <a href="/granttables/<%= granttable.id %>" class="btn btn-block btn-primary">Show More Details</a>
        <a href="/granttables/<%= granttable.id %>/edit" class="btn btn-block btn-warning">Edit</a>
      </tr>
      <%= end %>
    </table>
  </div>
</div>
