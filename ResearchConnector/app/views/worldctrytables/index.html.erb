<div class="page-header">
  <h1>
    List of World Countries
    <br>
    <br>
    <a href="/worldctrytables/new" class="btn btn-lg btn-block btn-success">New Worldctrytable</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Country ID</th>
        <th>Country Name</th>
        <th>Actions</th>
      </tr>

      <% @worldctrytables.each do |worldctrytable| %>
      <tr>
        <td><%= worldctrytable.COR_id %></td>
        <td><%= worldctrytable.COR %></td>
        <!-- <td><%= worldctrytable.COR_foreignkey %></td> -->
        <td>
          <a href="/worldctrytables/<%= worldctrytable.id %>" class="btn btn-primary">Show</a>
          <a href="/worldctrytables/<%= worldctrytable.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_worldctrytable/<%= worldctrytable.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
