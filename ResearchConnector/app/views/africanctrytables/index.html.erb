<div class="page-header">
  <h1>
    List of all African Countries
    <br>
    <br>
    <a href="/africanctrytables/new" class="btn btn-lg btn-block btn-success">New African Country</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Country ID</th>
        <th>Country Name</th>
        <th>Foreign Key</th>
        <th>Actions</th>
      </tr>

      <% @africanctrytables.each do |africanctrytable| %>
      <tr>
        <td><%= africanctrytable.AAC_id %></td>
        <td><%= africanctrytable.AAC %></td>
        <td><%= africanctrytable.AAC_foreignkey %></td>
        <td>
          <a href="/africanctrytables/<%= africanctrytable.id %>" class="btn btn-primary">Show</a>
          <a href="/africanctrytables/<%= africanctrytable.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_africanctrytable/<%= africanctrytable.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
